{
  "name": "blogpress",
  "version": "1.0.0",
  "description": "Blogging application",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "dev:start-server": "node-dev .",
    "dev:css-watch": "tailwind -c ./tailwindcss/tailwind.config.cjs -i ./tailwindcss/tailwind.css -o ./public/stylesheets/style.css --watch",
    "dev:start-db": "docker run -d --rm -p3306:3306 -e MYSQL_ROOT_PASSWORD=mysql_root --name db mysql",
    "dev:setup-db": "node ./db/setup.js",
    "dev:migrate-db": "node ./db/migrate.js",
    "dev:seed-db": "sequelize db:seed:all --seeders-path db/seeders --config db/config.json",
    "dev:format": "prettier --write ."
  },
  "keywords": [],
  "author": "Ernest Misha",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.1.3",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.4",
    "sequelize": "^6.17.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "node-dev": "^7.4.3",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.5.2",
    "tailwindcss": "^3.0.23"
  }
}
